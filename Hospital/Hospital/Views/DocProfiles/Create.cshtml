@model DocProfile

<h1>Create Doctor Profile</h1>

<form asp-action="Create" method="post" class="form-horizontal">

    <div class="form-group">
        <label asp-for="Picture">Picture:</label>
        <div class="row">
            <div class="col-md-5">
                <form method="post" enctype="multipart/form-data" asp-controller="DocProfiles" asp-action="Create">
                    <div class="form-group">
                        <div class="col-md-10">
                            <p>Upload Picture</p>
                            <input class="form-control" name="Picture" type="file" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="Degrees">Degrees:</label>
                        <div>
                            <input asp-for="Degrees" />
                        </div>
                    </div>

                    <div class="form-group">
                        <label asp-for="Awards">Awards:</label>
                        <div>
                            <input asp-for="Awards" />
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-10">
                            <input class="btn btn-primary" type="submit" value="Upload" />
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</form>

